 <div class="app-wrapper">
      <!-- row -->
      <div class="container-fluid">
          <div class="row my-5">
              <div class="col-12 py-5">
                  <div class="card py-4">
                      <div class="card-body">
                        
                          <h4 class="card-title pb-3">ALL PRODUCTS</h4>
                          {{#if user}}
                          <p class="text-danger">{{user}} has been successfully deleted!</p>
                          {{/if }}
                          <div class="table-responsive">
                              <table class="table table-striped table-bordered zero-configuration" id="myTable">
                                  <thead>
                                      <tr>
                                          <th>Images</th>
                                          <th>Product Title</th>
                                          <th>Brand</th>
                                          <th>Gender</th>
                                          <th>Category</th>
                                          <th>Price</th>
                                          <th>MRP</th>
                                          <th>Discount</th>
                                          <th>Sizes</th>
                                          <th>More ProductInfo</th>
                                          <th>Colors</th>
                                          <th>Stock</th>
                                          <th>Created At</th>
                                          <th>Updated At</th>
                                          <th>IsDeletedBy</th>
                                          <th>IsDeleted</th>
                                          <th>Action</th>
                                      </tr>
                                  </thead>
                                   
                                  <tbody>
                                      {{#each products}}
                                        <tr>
                                           <td>            
                                              <div class="carousel slide" data-bs-ride="carousel" id="carousel{{@index}}">
                                                  <div class="carousel-inner">
                                                      {{#each this.images}}
                                                          <div class="carousel-item {{#if @first}}active{{/if}}">
                                                              <img src="/uploads/{{this}}" class="d-block w-100" alt="Image {{@index}}">
                                                          </div>
                                                      {{/each}}
                                                  </div>
                                                  <button class="carousel-control-prev" type="button" data-bs-target="#carousel{{@index}}" data-bs-slide="prev">
                                                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                                      <span class="visually-hidden">Previous</span>
                                                  </button>
                                                  <button class="carousel-control-next" type="button" data-bs-target="#carousel{{@index}}" data-bs-slide="next">
                                                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                                      <span class="visually-hidden">Next</span>
                                                  </button>
                                              </div>  
                                           </td>
                                           <td>{{this.title}}</td>
                                           <td>{{this.brand.brand}}</td>
                                           <td>{{this.gender}}</td>
                                           <td>{{this.category.category}}</td>
                                           <td>₹{{this.price}}</td>
                                           <td>₹{{this.mrp}}</td>
                                           <td>{{this.discount}}%</td>
                                           <td>
                                               <div class="accordion border-0" id="sizes{{@index}}">
                                                   <div class="accordion-item">
                                                     <h2 class="accordion-header">
                                                       <button class="accordion-button" type="button" data-bs-toggle="collapse" 
                                                          data-bs-target="#sizecollapse{{@index}}" aria-expanded="true" aria-controls="collapseOne">
                                                          Sizes
                                                       </button>
                                                     </h2>
                                                     <div id="sizecollapse{{@index}}" class="accordion-collapse collapse" data-bs-parent="#sizes{{@index}}">
                                                       <div class="accordion-body">
                                                         <table class="table">
                                                           <thead>
                                                             <tr>
                                                               <th>Size</th>
                                                               <th>Stocks</th>
                                                             </tr>
                                                           </thead>
                                                           <tbody>
                                                             {{#each this.sizes}}
                                                               <tr>
                                                                 <td>{{@key}}</td>
                                                                 <td>{{calculateTotal this}}</td>
                                                               </tr>
                                                             {{/each}}
                                                           </tbody>
                                                         </table>
                                                       </div>
                                                     </div>
                                                   </div>
                                                </div>
                                           </td>
                                           <td>
                                                <div class="accordion border-0" id="details{{@index}}">
                                                   <div class="accordion-item">
                                                     <h2 class="accordion-header">
                                                       <button class="accordion-button" type="button" data-bs-toggle="collapse" 
                                                          data-bs-target="#collapse{{@index}}" aria-expanded="true" aria-controls="collapseOne">
                                                         Product Details
                                                       </button>
                                                     </h2>
                                                     <div id="collapse{{@index}}" class="accordion-collapse collapse" data-bs-parent="#details{{@index}}">
                                                       <div class="accordion-body">
                                                         <table class="table">
                                                           <thead>
                                                             <tr>
                                                               <th>Key</th>
                                                               <th>Value</th>
                                                             </tr>
                                                           </thead>
                                                           <tbody>
                                                             {{#each this.moreProductInfo}}
                                                               <tr>
                                                                 <td>{{@key}}</td>
                                                                 <td>{{this}}</td>
                                                               </tr>
                                                             {{/each}}
                                                           </tbody>
                                                         </table>
                                                       </div>
                                                     </div>
                                                   </div>
                                                </div>
                                           </td>
                                           <td>{{uniqueColors this.sizes}}</td>
                                           <td>{{this.totalStock}}</td>
                                           <td>{{removeGMT this.createdAt}}</td>
                                           <td>{{removeGMT this.updatedAt}}</td>
                                           <td>{{this.isDeletedBy}}</td>
                                           <td>{{this.isDeleted}}</td>
                                           <td>
                                                <a href="/admin/edit-product/{{this._id}}">
                                                    <i class="fa-solid fa-pen-to-square text-primary"></i>
                                                </a>
                                                {{#if this.isDeleted}}
                                                    <a href="/admin/restore-product/{{this._id}}">
                                                       <i class="fa-solid fa-rotate-left text-primary"></i></i>
                                                    </a>
                                                {{else}}
                                                    <a href="/admin/delete-product/{{this._id}}" class="deletebtn">
                                                        <i class="fa-solid fa-trash text-danger"></i>
                                                    </a>
                                                {{/if}}
                                           </td>
                                      </tr>
                                     {{/each}}
                                  </tbody>    
                              </table>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
      <!--  container -->
  </div>

 
