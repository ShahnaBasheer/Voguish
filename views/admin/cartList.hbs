 <div class="app-wrapper">
      <!-- row -->
      <div class="container-fluid">
          <div class="row my-5">
              <div class="col-12 py-5">
                  <div class="card py-4">
                      <div class="card-body">
                        
                          <h4 class="card-title pb-3">ALL USERS</h4>
                          {{#if user}}
                          <p class="text-danger">{{user}} has been successfully deleted!</p>
                          {{/if }}
                          {{#if flash}}
                             <div class="alert alert-success">
                                 {{flash.success}}
                             </div>
                          {{/if}}
                           
                          <div class="table-responsive">
                              <table class="table table-striped table-bordered zero-configuration" id="myTable">
                                  <thead>
                                      <tr>
                                          <th>UserName</th>
                                          <th>Items</th>
                                          <th>Created At</th>
                                          <th>Updated At</th>
                                          <th>Action</th>
                                      </tr>
                                  </thead>
                                  <tbody>
                                        <tr>
                                           <td></td>
                                           <td></td>
                                           <td></td>
                                           <td></td>
                                           <td></td>
                                        </tr>
                                  </tbody> 
                              </table>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
      <!--  container -->
  </div>

 