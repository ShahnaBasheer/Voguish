 <div class="app-wrapper">
      <!-- row -->
      <div class="container-fluid">
          <div class="row my-5">
              <div class="col-12 py-5">
                  <div class="card py-4">
                      <div class="card-body">
                        
                          <h4 class="card-title pb-3">ALL USERS</h4>
                          {{#if user}}
                          <p class="text-danger">{{user}} has been successfully deleted!</p>
                          {{/if }}
                          {{#if flash}}
                             <div class="alert alert-success">
                                 {{flash.success}}ss
                             </div>
                          {{/if}}
                           
                          <div class="table-responsive">
                              <table class="table table-striped table-bordered zero-configuration" id="myTable">
                                  <thead>
                                      <tr>
                                          <th>Name</th>
                                          <th>Gender</th>
                                          <th>Email</th>
                                          <th>Phone</th>
                                          <th>Addresses</th>
                                          <th>Wishlist</th>
                                          <th>isBlocked</th>
                                          <th>Created At</th>
                                          <th>Updated At</th>
                                          <th>Action</th>
                                      </tr>
                                  </thead>
                                   
                                  <tbody>
                                   {{#each users}}
                                      {{#unless (roleEquals this.role)}}
                                  
                                      <tr>
                                          <td>{{this.firstname}} {{this.lastname}} </td>
                                          <td>{{this.gender}}</td>
                                          <td>{{this.email}}</td>
                                          <td>{{this.phone}}</td>
                                          <td>{{#each this.addresses}}
                                              <div>
                                                {{inc @index}}. {{this.address}}
                                              </div>
                                            {{/each}}

                                          </td>
                                          <td>{{this.wishlist}}</td>
                                          <td>  
                                            
                                              <div class="form-check form-switch"> 
                                                  <input class="form-check-input flexSwitchCheck" type="checkbox"
                                                      {{#if this.isBlocked }} checked
                                                      {{ else }} {{/if}} 
                                                  
                                                      role="switch" data-id = {{this._id}}/>
                                                      {{#if this.isBlocked }} <span>Blocked</span>
                                                      {{ else }} {{/if}}
                                                  <span></span>
                                              </div>
                                             
                                          </td>
                                          <td>{{removeGMT this.createdAt}}</td>
                                          <td>{{removeGMT this.updatedAt}}</td>
                                          <td>
                                              <a href="delete-user/{{this._id}}" 
                                                 class="btn btn-danger deletebtn">Delete</a>
                                          </td>
                                      </tr>
                                      {{/unless}}
                                    {{/each}} 
                                   
                                  </tbody>
                                  
                              </table>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
      <!--  container -->
  </div>

 